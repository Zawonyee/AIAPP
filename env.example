# 重要：使用前请重命名此文件为.env并填写您的API密钥

# API配置
DASHSCOPE_API_KEY=your_api_key_here
MODEL_NAME=qwen-vl-max-0809

# 视频处理配置
FRAME_SKIP=3
MOTION_THRESHOLD=1000
MIN_OBJECT_AREA=500
API_CALL_INTERVAL=1.1
API_TIMEOUT=10

# 输出配置
OUTPUT_DIR=./results
SAVE_TRACKING_VIDEO=false
OUTPUT_CODEC=mp4v

# 性能配置
USE_GPU=false
MAX_CACHE_SIZE=1024
TRACK_HISTORY=30

# 数据库配置（可选）
# DB_HOST=localhost
# DB_PORT=3306
# DB_USER=root
# DB_PASSWORD=password
# DB_NAME=video_analysis
# RESET_DB=false

# 调试级别：DEBUG, INFO, WARNING, ERROR
DEBUG_LEVEL=INFO 